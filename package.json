{
  "name": "tsv2md",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "https://github.com/AKPWebDesign/TSV2MD.git",
  "author": "Austin Peterson <austin@akp.software>",
  "license": "MIT",
  "browserslist": [
    "> 0.2%",
    "not dead"
  ],
  "scripts": {
    "build": "parcel build public/index.html",
    "clean": "rimraf dist",
    "deepclean": "rimraf dist .parcel-cache",
    "lint": "eslint 'src/**/*.{js,ts,tsx}' --quiet --fix",
    "powerwash": "rimraf dist .parcel-cache node_modules && yarn",
    "start": "parcel public/index.html",
    "typecheck": "tsc --noEmit"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsc --noEmit && lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix"
    ]
  },
  "devDependencies": {
    "@types/node": "^14.14.37",
    "@types/parcel-env": "^0.0.0",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.3",
    "@typescript-eslint/eslint-plugin": "^4.21.0",
    "@typescript-eslint/parser": "^4.21.0",
    "eslint": "^7.23.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.23.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "parcel": "^2.0.0-nightly.634",
    "rimraf": "^3.0.2",
    "typescript": "^4.2.3"
  },
  "dependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-markdown": "^5.0.3",
    "remark-gfm": "^1.0.0"
  }
}
